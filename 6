programa
{
	const inteiro TAMANHO = 6
	inteiro meuVetor[TAMANHO]
	funcao inteiro encontrarMenor(inteiro vetorInteiros[], inteiro n) 
	{
		inteiro j 
		inteiro menor = vetorInteiros[0] 

		para (j = 1; j < n; j++)
		{
			se (vetorInteiros[j] < menor)
			{
				menor = vetorInteiros[j]
			}
		}

		retorne menor
	}

	funcao inicio()
	{
		inteiro menorValor

		meuVetor[0] = 45
		meuVetor[1] = 12
		meuVetor[2] = 88 
		meuVetor[3] = 30
		meuVetor[4] = 71
		meuVetor[5] = 5 
		
		escreva("Vetor de entrada: [45, 12, 88, 30, 71, 5]\n")

		menorValor = encontrarMenor(meuVetor, TAMANHO)

		escreva("O menor valor presente no vetor Ã©: ", menorValor, "\n")
	}
}
